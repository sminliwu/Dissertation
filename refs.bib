  month    = jul,
  year     = {2019},
  keywords = {Design advisory system, Knowledge based advisory system, Multi material joining, databases},
  pages    = {253--263},
  file     = {ScienceDirect Snapshot:C\:\\Users\\Shanel Wu\\Zotero\\storage\\FXMRQGQR\\S0278612519300135.html:text/html}
}

@inproceedings{kim_machines_2017,
  address    = {New York, NY, USA},
  series     = {{CHI} {EA} '17},
  title      = {Machines as {Co}-{Designers}: {A} {Fiction} on the {Future} of {Human}-{Fabrication} {Machine} {Interaction}},
