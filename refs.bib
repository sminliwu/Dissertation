  month      = may,
  year       = {2017},
  keywords   = {creativity support, emerging design, concurrent collaboration, human-fabmachine interaction},
  pages      = {790--805},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\H8Z6D857\\Kim et al. - 2017 - Machines as Co-Designers A Fiction on the Future .pdf:application/pdf}
}

@inproceedings{kim_shall_2017,
