  month      = oct,
  year       = {2021},
  keywords   = {e-textiles, haptics, microfluidics, soft actuator, movement-based HCI, stretchable sensor},
  pages      = {1010--1026},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\PRTDN9Q5\\Kilic Afsar et al. - 2021 - OmniFiber Integrated Fluidic Fiber Actuators for .pdf:application/pdf}
