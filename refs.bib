  author     = {Oogjes, Doenja and Wakkary, Ron},
  month      = apr,
  year       = {2022},
  keywords   = {weaving, designing things, posthuman design, more-than-human, posthumanism, repertoires, first-person, research through design, e-textiles},
  pages      = {1--21},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\GI9QG22L\\Oogjes and Wakkary - 2022 - Weaving Stories Toward Repertoires for Designing .pdf:application/pdf}
