@inproceedings{jacobs_codeable_2013,
  address    = {New York, NY, USA},
  series     = {{CHI} '13},
  title      = {Codeable {Objects}: {Computational} {Design} and {Digital} {Fabrication} for {Novice} {Programmers}},
