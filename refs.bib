  month     = sep,
  year      = {2013},
  keywords  = {playfulness, heuristic evaluation, evaluation methods},
  pages     = {221--230},
  file      = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\BB44GWBE\\Lucero et al. - 2013 - The playful experiences (PLEX) framework as a guid.pdf:application/pdf}
}

@inproceedings{ludwig_towards_2014,
