  month      = oct,
  year       = {2021},
  keywords   = {hardware description language (HDL), integrated development environment (IDE), printed circuit board (PCB) design},
  pages      = {1039--1049},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\EUM6EEJM\\Lin et al. - 2021 - Weaving Schematics and Code Interactive Visual Ed.pdf:application/pdf}
