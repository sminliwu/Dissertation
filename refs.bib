@book{massumi_users_1992,
  address    = {Cambridge, Mass},
  edition    = {A Swerve Ed edition},
  title      = {A {User}'s {Guide} to {Capitalism} and {Schizophrenia}: {Deviations} from {Deleuze} and {Guattari}},
  isbn       = {978-0-262-63143-3},
  shorttitle = {A {User}'s {Guide} to {Capitalism} and {Schizophrenia}},
  abstract   = {A User's Guide to Capitalism and Schizophrenia is a playful and emphatically practical elaboration of the major collaborative work of the French philosophers Gilles Deleuze and Felix Guattari. When read along with its rigorous textual notes, the book also becomes the richest scholarly treatment of Deleuze's entire philosophical oeuvre available in any language. Finally, the dozens of explicit examples that Brian Massumi furnishes from contemporary artistic, scientific, and popular urban culture make the book an important, perhaps even central text within current debates on postmodern culture and politics.Capitalism and Schizophrenia is the general title for two books published a decade apart. The first, Anti-Oedipus, was a reaction to the events of May/June 1968; it is a critique of "state-happy" Marxism and "school-building" strains of psychoanalysis. The second, A Thousand Plateaus, is an attempt at a positive statement of the sort of nomad philosophy Deleuze and Guattari propose as an alternative to state philosophy.Brian Massumi is Professor of Comparative Literature at McGill University.},
  language   = {English},
  publisher  = {The MIT Press},
  author     = {Massumi, Brian},
  month      = mar,
  year       = {1992}
}

@article{mastos_introducing_2021,
  title    = {Introducing an application of an industry 4.0 solution for circular supply chain management},
  volume   = {300},
  issn     = {0959-6526},
  url      = {https://www.sciencedirect.com/science/article/pii/S0959652621011057},
  doi      = {10.1016/j.jclepro.2021.126886},
  abstract = {In recent years, sustainable supply chain management practices have been adopted by companies that desire to reduce the negative environmental and social impacts within their supply chains. Within this perspective, a circular approach has been developed in the supply chain literature. Circular economy models and solutions assisted by industry 4.0 technologies have been developed to transform products in the end of their life cycle into new products with different use. In this paper an industry 4.0 waste-to-energy solution is developed and applied in a pilot case study comprised by a real-world supply chain to evaluate the sustainability performance of circular supply chain management (CSCM). The findings show that redesigning supply chains for circular economy with the use of Industry 4.0 technologies, can enable circular supply chain management. Clear benefits are provided linking the proposed solution to the six circular economy dimensions of the ReSOLVE model i.e. regenerate, share, optimise, loop, virtualise, and exchange. Improved availability of personnel (5\% and 15\%) and fleet resources (15\%) are identified as some of the key quantitative benefits, while supply chain traceability through the full visibility and automation offered by the proposed solution, are some of the key non-quantifiable outcomes. The present work seeks to contribute to the existing literature by providing empirical evidence of how industry 4.0 and circular economy are applied in practice. Implications for managers and policy makers, along with the study limitations and further research paths are also presented.},
  language = {en},
  urldate  = {2021-05-12},
  journal  = {Journal of Cleaner Production},
  author   = {Mastos, Theofilos D. and Nizamis, Alexandros and Terzi, Sofia and Gkortzis, Dimitrios and Papadopoulos, Angelos and Tsagkalidis, Nikolaos and Ioannidis, Dimosthenis and Votis, Konstantinos and Tzovaras, Dimitrios},
  month    = jun,
  year     = {2021},
  keywords = {CE (circular economy), Industry 4.0, CSCM (circular supply chain management), ReSOLVE model, Waste-to-energy},
  pages    = {126886},
  file     = {ScienceDirect Snapshot:C\:\\Users\\sminl\\Zotero\\storage\\MIBR5GMS\\S0959652621011057.html:text/html}
}

