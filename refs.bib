@inproceedings{dementyev_sensortape:_2015,
  address    = {New York, NY, USA},
  series     = {{UIST} '15},
  title      = {{SensorTape}: {Modular} and {Programmable} 3D-{Aware} {Dense} {Sensor} {Network} on a {Tape}},
