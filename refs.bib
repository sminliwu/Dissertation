  month     = jan,
  year      = {2020},
  doi       = {10.1016/B978-0-12-818782-1.00004-3},
  keywords  = {Economic value of flax, Flax and hemp fibres, Textile application, Trends for flax and hemp},
  pages     = {147--167},
  file      = {ScienceDirect Full Text PDF:C\:\\Users\\Shanel Wu\\Zotero\\storage\\QTLY2KGA\\Muzyczek - 2020 - 4 - The use of flax and hemp for textile applicati.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Shanel Wu\\Zotero\\storage\\BQGG5NTY\\B9780128187821000043.html:text/html}
