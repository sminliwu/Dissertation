  edition    = {Illustrated edition},
  title      = {Mending {Life}: {A} {Handbook} for {Mending} {Clothes} and {Hearts} (with {Basic} {Stitching}, {Sashiko}, {Darnin} g, and {Patching} to {Practice} {Sustainable} {Fashion} and {Repair} the {Clothes} {You} {Love}},
