  pages      = {1--5},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\QSU2YL54\\Fraser et al. - 2019 - Digital -is- Physical How Functional Fabrication .pdf:application/pdf}
}

@inproceedings{fraser_sharing_2019,
  address    = {New York, NY, USA},
