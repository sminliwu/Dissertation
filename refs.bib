@inproceedings{carlson_ewaste-worker_2016,
  author    = {Carlson, Krystin},
  title     = {Electronic Waste Worker Health: Studying Complex Problems in the Real World},
  year      = {2016},
  isbn      = {9781450343060},
  publisher = {Association for Computing Machinery},
  address   = {New York, NY, USA},
  url       = {https://doi-org.colorado.idm.oclc.org/10.1145/2909609.2909618},
  doi       = {10.1145/2909609.2909618},
  abstract  = {Electronic waste (e-waste) is a growing problem in both industrialized and non-industrialized
               societies. Devices that users interact with innocuously each day contain hazardous
               materials such as lead and cadmium. These constituents become released into workers
               and the environment as electronic devices are broken down into recyclable components.
               The stream of e-waste targets globally marginalized communities where informal workers
               use straightforward and low-tech options, such as burning and pounding, to dismantle
               waste. Complex international problems like e-waste encompass many difficult issues,
               but also offer opportunities for researchers to embrace global perspectives and dynamic
               challenges. This paper explores my work researching e-waste worker health. Accepted
               paradigms in research on occupational health and solving global health issues have
               been challenged throughout my project. I have observed how research approaches are
               often biased toward a Western situation and how disadvantageous this can be to vulnerable
               populations in marginalized communities. By participating in a project exploring the
               health of Ghanaian e-waste workers, I grew as a researcher, as a health scientist,
               and as a global community member. I offer observations and reflections of this research
               experience hoping they will bring insight into understanding the issue of e-waste
               and possibilities for future work across the globe confronting this toxic threat.},
  booktitle = {Proceedings of the Eighth International Conference on Information and Communication Technologies and Development},
  articleno = {35},
  numpages  = {4},
  keywords  = {Ghana, Noise, International Research, Lead, Agbogbloshie, Graduate Study, Public Health, Epidemiology, Occupational Health, E-waste, Global Health},
  location  = {Ann Arbor, MI, USA},
  series    = {ICTD '16}
}

