@article{wu_weavecraft_2020,
  title      = {Weavecraft: an interactive design and simulation tool for {3D} weaving},
  volume     = {39},
  issn       = {0730-0301},
  shorttitle = {Weavecraft},
  url        = {https://doi.org/10.1145/3414685.3417865},
  doi        = {10.1145/3414685.3417865},
  abstract   = {3D weaving is an emerging technology for manufacturing multilayer woven textiles. In this work, we present Weavecraft: an interactive, simulation-based design tool for 3D weaving. Unlike existing textile software that uses 2D representations for design patterns, we propose a novel weave block representation that helps the user to understand 3D woven structures and to create complex multi-layered patterns. With Weavecraft, users can create blocks either from scratch or by loading traditional weaves, compose the blocks into large structures, and edit the pattern at various scales. Furthermore, users can verify the design with a physically based simulator, which predicts and visualizes the geometric structure of the woven material and reveals potential defects at an interactive rate. We demonstrate a range of results created with our tool, from simple two-layer cloth and well known 3D structures to a more sophisticated design of a 3D woven shoe, and we evaluate the effectiveness of our system via a formative user study.},
  number     = {6},
  urldate    = {2022-08-31},
  journal    = {ACM Transactions on Graphics},
  author     = {Wu, Rundong and Zhang, Joy Xiaoji and Leaf, Jonathan and Hua, Xinru and Qu, Ante and Harvey, Claire and Holtzman, Emily and Ko, Joy and Hagan, Brooks and James, Doug and Guimbretière, François and Marschner, Steve},
  month      = nov,
  year       = {2020},
  keywords   = {3D weaving, computer aided design, yarn-level cloth simulation},
  pages      = {210:1--210:16},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\GE59PRTC\\Wu et al. - 2020 - Weavecraft an interactive design and simulation t.pdf:application/pdf}
}

@inproceedings{wu_what_2019,
  address    = {Glasgow, Scotland Uk},
  series     = {{CHI} '19},
  title      = {"{What} is {Fair} {Shipping}, {Anyway}?": {Using} {Design} {Fiction} to {Raise} {Ethical} {Awareness} in an {Industrial} {Context}},
  isbn       = {978-1-4503-5970-2},
  shorttitle = {"{What} is {Fair} {Shipping}, {Anyway}?},
  url        = {http://doi.org/10.1145/3290605.3300666},
  doi        = {10.1145/3290605.3300666},
  abstract   = {The HCI community cares for the human and social aspects of technologies. Ethical discussion on the social implications of new technologies often happen among researchers, but it is important to raise this discussion also in the industry that designs and implements new systems. In this paper, we introduce a case in which design fiction was used as an ethical discussion tool among company partners. We report the process of creating and prototyping a fictional world embedded with conflicting values that aimed to shift the focus from industrial merits towards societal values and raise discussion among participants. Moreover, we examine the challenges and propose suggestions in crafting critiques and friction to the industrial context. Our findings suggest why and how one should use design fiction as a means to raise ethical awareness in a technology- and profit-focused context, to support further activities on developing more humane technological futures.},
  urldate    = {2020-07-30},
  booktitle  = {Proceedings of the 2019 {CHI} {Conference} on {Human} {Factors} in {Computing} {Systems}},
  publisher  = {Association for Computing Machinery},
  author     = {Wu, Yiying and Lyckvi, Sus and Roto, Virpi},
  month      = may,
  year       = {2019},
  keywords   = {design methods, design fiction, ethics, design methodology, ethics of automation, value fiction, automation, B2B industries},
  pages      = {1--13}
}

