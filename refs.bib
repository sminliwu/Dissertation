}

@inproceedings{albaugh_investigating_2020,
  address   = {New York, NY, USA},
  series    = {{DIS} '20},
  title     = {Investigating {Underdetermination} {Through} {Interactive} {Computational} {Handweaving}},
  isbn      = {978-1-4503-6974-9},
  url       = {https://doi.org/10.1145/3357236.3395538},
  doi       = {10.1145/3357236.3395538},
  abstract  = {Computational handweaving combines the repeatable precision of digital fabrication with relatively high production demands of the user: a weaver must be physically engaged with the system to enact a pattern, line by line, into a fabric. Rather than approaching co-presence and repetitive labor as a negative aspect of design, we look to current practices in procedural generation (most commonly used in game design and screen-based new media art) to understand how designers can create room for suprise and emergent phenomena within systems of precision and constraint. We developed three designs for blending real-time input with predetermined pattern features. These include: using camera imagery sampled at weaving time; a 1:1 scale tool for composing patterns on the loom; and a live "Twitch'' stream where spectators determine the woven pattern. We discuss how experiential qualities of the systems led to different balances of underdetermination in procedural generation as well as how such an approach might help us think beyond an artifact/experience dichotomy in fabrication.},
  urldate   = {2022-08-31},
  booktitle = {Proceedings of the 2020 {ACM} {Designing} {Interactive} {Systems} {Conference}},
  publisher = {Association for Computing Machinery},
  author    = {Albaugh, Lea and Hudson, Scott E. and Yao, Lining and Devendorf, Laura},
  month     = jul,
  year      = {2020},
  keywords  = {additive manufacturing, computational crafts, hybrid fabrication, interactive fabrication, procedural design, soft materials},
  pages     = {1033--1046}
}

