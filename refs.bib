  pages      = {110--119},
  file       = {ScienceDirect Full Text PDF:C\:\\Users\\Shanel Wu\\Zotero\\storage\\5WVHGBJZ\\Azab et al. - 2013 - Mechanics of change A framework to reconfigure ma.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Shanel Wu\\Zotero\\storage\\CGFXRVC3\\S1755581712000958.html:text/html}
