  pages      = {1462--1484},
  file       = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\79KX6H4K\\Machado et al. - 2020 - Sustainable manufacturing in Industry 4.0 an emer.pdf:application/pdf;Snapshot:C\:\\Users\\sminl\\Zotero\\storage\\AIJ9KZMH\\00207543.2019.html:text/html}
