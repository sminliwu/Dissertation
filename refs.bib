  year      = {2007},
  keywords  = {design, interaction design, research through design, design theory, design method, HCI research, interaction design research, wicked problems},
  pages     = {493--502},
  file      = {Full Text PDF:C\:\\Users\\sminl\\Zotero\\storage\\ZEBASG2M\\Zimmerman et al. - 2007 - Research through design as a method for interactio.pdf:application/pdf}
} 

